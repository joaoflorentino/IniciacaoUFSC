@article{Kondo:15,
author = {Jorge M. Kondo and Nikola \v{S}ibali\'{c} and Alexander Guttridge and Christopher G. Wade and Natalia R. De Melo and Charles S. Adams and Kevin J. Weatherill},
journal = {Opt. Lett.},
keywords = {Coherent optical effects; Hyperfine structure; Multiphoton processes; Rydberg states; Optical memories},
number = {23},
pages = {5570--5573},
publisher = {OSA},
title = {Observation of interference effects via four-photon excitation of highly excited Rydberg states in thermal cesium vapor},
volume = {40},
month = {Dec},
year = {2015},
url = {http://ol.osa.org/abstract.cfm?URI=ol-40-23-5570},
doi = {10.1364/OL.40.005570},
abstract = {We report on the observation of electromagnetically induced transparency (EIT) and absorption (EIA) of highly excited Rydberg states in thermal Cs vapor using a four-step excitation scheme. The advantage of this four-step scheme is that the final transition to the Rydberg state has a large dipole moment and one can achieve similar Rabi frequencies to two- or three-step excitation schemes using two orders of magnitude less laser power. This scheme enables new applications such as dephasing free Rydberg excitation. The observed lineshapes are in good agreement with simulations based on multilevel optical Bloch equations.},
}


@article{jader2011,
  author={J S Cabral and J M Kondo and L F Gonçalves and V A Nascimento and L G Marcassa and D Booth and J Tallant and A Schwettmann and K R
Overstreet and J Sedlacek and J P Shaffer},
  title={Effects of electric fields on ultracold Rydberg atom interactions},
  journal={Journal of Physics B: Atomic, Molecular and Optical Physics},
  volume={44},
  number={18},
  pages={184007},
  url={http://stacks.iop.org/0953-4075/44/i=18/a=184007},
  year={2011},
  abstract={The behaviour of interacting ultracold Rydberg atoms in both constant electric fields and laser fields is important for designing experiments and constructing realistic models of them. In this paper, we briefly review our prior work and present new results on how electric fields affect interacting ultracold Rydberg atoms. Specifically, we address the topics of constant background electric fields on Rydberg atom pair excitation and laser-induced Stark shifts on pair excitation.}
}

@article{jader2010,
  author={J S Cabral and J M Kondo and L F Gonçalves and L G Marcassa and D Booth and J Tallant and J P Shaffer},
  title={Manipulation of quantum state transfer in cold Rydberg atom collisions},
  journal={New Journal of Physics},
  volume={12},
  number={9},
  pages={093023},
  url={http://stacks.iop.org/1367-2630/12/i=9/a=093023},
  year={2010},
  abstract={We investigate the role of the dc Stark effect in multilevel pairwise interactions between cold Rydberg atoms. We have observed the decay of nD + nD quasi-molecules by detecting the products in the ( n +2)P state after pulsed excitation for 29≤ n ≤41. The decay rate can be manipulated with a dc electric field and requires a consideration of the multilevel nature of the process to explain the observations. The time dependence of the ( n +2)P signal is found to support a time-dependent picture of the dynamics.}
}	

